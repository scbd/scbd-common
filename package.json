{
  "name": "@scbd/common",
  "private": false,
  "version": "0.1.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/scbd/scbd-common.git"
  },
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "unpkg": "dist/index.umd.js",
  "jsdelivr": "dist/index.umd.js",
  "browser": "dist/index.umd.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.umd.js"
    },
    "./components/*": {
      "import": "./dist/components/*.js",
      "require": "./dist/components/*.cjs",
      "default": "./dist/components/*.umd.js"
    },
    "./services/*": {
      "import": "./dist/services/*.js",
      "require": "./dist/services/*.cjs",
      "default": "./dist/services/*.umd.js"
    }
  },
  "files": [
    "dist/*",
    "package.json",
    "LICENSE",
    "README.md"
  ],
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "rm -rf ./dist && rollup -c",
    "build:watch": "rm -rf ./dist && rollup -c --watch",
    "cleanup": "rm -rf ./dist && rm -rf ./node_modules && npm install",
    "cleanup:hard": "rm -rf ./dist && rm -rf ./node_modules && rm -rf ./package-lock.json",
    "test": "vitest"
  },
  "dependencies": {
    "vue": "^3.4.15"
  },
  "peerDependencies": {
    "axios": "^1.3.3",
    "lodash": "^4.17.21",
    "luxon": "^3.2.1",
    "require-css": "^0.1.10",
    "vue": "^3.4.15",
    "bootstrap": "^5.3.3",
    "bootstrap-icons": "^1.11.3",
    "webui-popover": "^1.2.18",
    "jquery": "^3.7.0", 
    "vue-multiselect": "^3.0.0-beta.3",
    "@ckeditor/ckeditor5-build-classic": "^41.2.1",
    "@ckeditor/ckeditor5-vue": "^5.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/preset-env": "^7.19.4",
    "@rollup/plugin-alias": "^5.1.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@vitejs/plugin-vue": "^5.0.3",
    "glob": "^8.0.3",
    "install": "^0.13.0",
    "npm": "^8.19.2",
    "rollup": "^2.79.1",
    "rollup-plugin-scss": "^4.0.0",
    "rollup-plugin-vue": "^6.0.0",
    "sass": "^1.70.0",
    "scss": "^0.2.4",
    "vite": "^5.0.11",
    "vitest": "^1.3.0"
  }
}