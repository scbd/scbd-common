{
  "name": "@scbd/common",
  "private": false,
  "version": "0.0.2",
  "repository": {
    "type": "git",
    "url": "https://github.com/scbd/scbd-common.git"
  },
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "unpkg": "dist/index.js",
  "jsdelivr": "dist/index.js",
  "browser": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./components/*": {
      "import": "./dist/components/*.mjs",
      "require": "./dist/components/*.cjs",
      "default": "./dist/components/*.js"
    },
    "./services/*": {
      "import": "./dist/services/*.mjs",
      "require": "./dist/services/*.cjs",
      "default": "./dist/services/*.js"
    }
  },
  "files": [
    "dist/*",
    "package.json",
    "LICENSE",
    "README.md"
  ],
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "rm -rf ./dist && rollup -c",
    "build:watch": "rm -rf ./dist && rollup -c --watch",
    "cleanup": "rm -rf ./dist && rm -rf ./node_modules",
    "cleanup:hard": "rm -rf ./dist && rm -rf ./node_modules && rm -rf ./package-lock.json"
  },
  "dependencies": {
    "vue": "^2.7.10"
  },
  "peerDependencies": {
    "vue": "^2.7.10"
  },
  "devDependencies": {
    "@rollup/plugin-buble": "^0.21.3",
    "@vitejs/plugin-vue2": "^2.0.0",
    "glob": "^8.0.3",
    "lodash": "^4.17.21",
    "rollup": "^2.79.1",
    "rollup-plugin-vue": "^5.1.9",
    "vite": "^3.1.0",
    "vue-template-compiler": "^2.7.10"
  }
}
