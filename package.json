{
  "name": "@scbd/common",
  "private": false,
  "version": "0.1.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/scbd/scbd-common.git"
  },
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "unpkg": "dist/index.umd.js",
  "jsdelivr": "dist/index.umd.js",
  "browser": "dist/index.umd.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.umd.js"
    },
    "./components/*": {
      "import": "./dist/components/*.js",
      "require": "./dist/components/*.cjs",
      "default": "./dist/components/*.umd.js"
    },
    "./services/*": {
      "import": "./dist/services/*.js",
      "require": "./dist/services/*.cjs",
      "default": "./dist/services/*.umd.js"
    }
  },
  "files": [
    "dist/*",
    "package.json",
    "LICENSE",
    "README.md"
  ],
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "rm -rf ./dist && rollup -c",
    "build:watch": "rm -rf ./dist && rollup -c --watch",
    "cleanup": "rm -rf ./dist && rm -rf ./node_modules",
    "cleanup:hard": "rm -rf ./dist && rm -rf ./node_modules && rm -rf ./package-lock.json",
    "test": "vitest"
  },
  "dependencies": {
    "jsdom": "^24.0.0",
    "vue": "^2.7.10"
  },
  "peerDependencies": {
    "axios": "^1.3.3",
    "lodash": "^4.17.21",
    "luxon": "^3.2.1",
    "require-css": "^0.1.10",
    "vue": "^2.7.10"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/preset-env": "^7.19.4",
    "@rollup/plugin-alias": "^4.0.3",
    "@rollup/plugin-babel": "^6.0.0",
    "@vitejs/plugin-vue2": "^2.0.0",
    "glob": "^8.0.3",
    "install": "^0.13.0",
    "lodash": "*",
    "npm": "^8.19.2",
    "rollup": "^2.79.1",
    "rollup-plugin-vue": "^5.1.9",
    "vite": "^3.1.0",
    "vitest": "^1.2.2",
    "vue-template-compiler": "^2.7.10"
  }
}
